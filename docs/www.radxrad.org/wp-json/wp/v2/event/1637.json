irecturl=\"https:\/\/www.google.com\/url?q=https:\/\/urldefense.proofpoint.com\/v2\/url?u%3Dhttps-3A__osf.io_ef53g_%26d%3DDwMFAg%26c%3DbKRySV-ouEg_AT-w2QWsTdd9X__KYh9Eq2fdmQDVZgw%26r%3DB6xQ-aLVzYGsDjZmmahOIfePOQbP5xbbd9eG1jV7D44%26m%3DEDNZjUEC_kE95kuZj6-7QlHGPsX1qqL6rZB9TbJOqeA%26s%3DLZlJ7dFIawr_X9cZmk4AcwXHqlwVVdOwdxZwbbmHzb4%26e%3D&amp;source=gmail&amp;ust=1621973650774000&amp;usg=AFQjCNEDOMubYXbarPabKvf94uaOU4032w\">poster templates<\/a>, Why not try them out? \u00a0<\/span><\/p>\n<p><span lang=\"EN-GB\">Please come and join us. reserve the dates, share this message and encourage your science associates to attend.<\/span><\/p>\n<p>&nbsp;<\/p>\n","protected":false},"author":2,"menu_order":0,"template":"","meta":{"inline_featured_image":false,"footnotes":""},"foas":[],"event_category":[55],"class_list":["post-1637","event","type-event","status-publish","hentry","event_category-past-event"],"acf":[],"_links":{"self":[{"href":"https:\/\/www.radxrad.org\/wp-json\/wp\/v2\/event\/1637","targetHints":{"allow":["GET"]}}],"collection":[{"href":"https:\/\/www.radxrad.org\/wp-json\/wp\/v2\/event"}],"about":[{"href":"https:\/\/www.radxrad.org\/wp-json\/wp\/v2\/types\/event"}],"author":[{"embeddable":true,"href":"https:\/\/www.radxrad.org\/wp-json\/wp\/v2\/users\/2"}],"version-history":[{"count":2,"href":"https:\/\/www.radxrad.org\/wp-json\/wp\/v2\/event\/1637\/revisions"}],"predecessor-version":[{"id":1639,"href":"https:\/\/www.radxrad.org\/wp-json\/wp\/v2\/event\/1637\/revisions\/1639"}],"wp:attachment":[{"href":"https:\/\/www.radxrad.org\/wp-json\/wp\/v2\/media?parent=1637"}],"wp:term":[{"taxonomy":"foas","embeddable":true,"href":"https:\/\/www.radxrad.org\/wp-json\/wp\/v2\/foas?post=1637"},{"taxonomy":"event_category","embeddable":true,"href":"https:\/\/www.radxrad.org\/wp-json\/wp\/v2\/event_category?post=1637"}],"curies":[{"name":"wp","href":"https:\/\/api.w.org\/{rel}","templated":true}]}}